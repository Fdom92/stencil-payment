    "@stencil/core": "0.0.4"
  },
  "devDependencies": {
    "@stencil/dev-server": "0.0.16",
    "@stencil/utils": "0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fdom92/stencil-payment.git"
  },
